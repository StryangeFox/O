#include <stdio.h>

int main(int argc, char* argv[], char* env[]) {
	printf("Number of command line arguments: %i\n", argc);
	printf("Command line arguments:\n");
	for (int i = 0; i < argc; i++)
		printf("%s\n", argv[i]);
	printf("Environment states arguments:");
	for (int i = 0;env[i] != NULL; i++)
		printf("%s\n", env[i]);
	return 0;
}
